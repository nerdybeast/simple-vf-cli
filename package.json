{
  "name": "simple-vf-cli",
  "version": "0.8.8-beta-1",
  "description": "A simple cli tool for building visualforce pages locally using cutting edge front-end build systems.",
  "author": "therealnerdybeast@gmail.com",
  "main": "index.js",
  "preferGlobal": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nerdybeast/simple-vf-cli"
  },
  "keywords": [
    "salesforce",
    "visualforce",
    "visual-force",
    "cli",
    "simple"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "bin": {
    "svf": "./bin/run"
  },
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc",
    "prepublishOnly": "npm test && npm run build",
    "postinstall": "node ./dist/install",
    "publish:beta": "npm publish --tag=beta"
  },
  "dependencies": {
    "@nestjs/common": "^6.2.4",
    "@nestjs/core": "^6.2.4",
    "archiver": "2.1.0",
    "chalk": "2.3.0",
    "commander": "2.11.0",
    "cross-spawn": "5.1.0",
    "debug": "^4.1.1",
    "dotenv": "4.0.0",
    "fs-extra": "^8.0.1",
    "gulp-sourcemaps": "2.6.1",
    "inquirer": "^6.3.1",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jsforce": "^1.9.1",
    "keytar": "^4.9.0",
    "moment": "^2.24.0",
    "ngrok": "2.2.23",
    "node-json-db": "^0.11.0",
    "node-watch": "0.5.5",
    "ora": "1.3.0",
    "pouchdb": "6.3.4",
    "pouchdb-find": "6.3.4",
    "randomstring": "1.1.5",
    "reflect-metadata": "^0.1.13",
    "rollbar": "2.3.1",
    "rxjs": "^6.5.2",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/archiver": "2.0.1",
    "@types/commander": "2.11.0",
    "@types/debug": "^4.1.4",
    "@types/dotenv": "4.0.2",
    "@types/fs-extra": "^7.0.0",
    "@types/gulp": "3.8.34",
    "@types/inquirer": "0.0.35",
    "@types/jest": "^21.1.5",
    "@types/jsforce": "^1.9.6",
    "@types/ora": "1.3.1",
    "@types/pouchdb": "6.3.1",
    "@types/pouchdb-find": "6.3.1",
    "@types/randomstring": "1.1.6",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  },
  "jest": {
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "tests/.*\\.test\\.ts",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
