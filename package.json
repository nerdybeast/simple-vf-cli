{
  "name": "simple-vf-cli",
  "version": "0.5.1",
  "description": "A simple cli tool for building visualforce pages locally using cutting edge front-end build systems.",
  "author": "therealnerdybeast@gmail.com",
  "main": "index.js",
  "preferGlobal": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nerdybeast/simple-vf-cli"
  },
  "keywords": [
    "salesforce",
    "visualforce",
    "visual-force",
    "cli",
    "simple"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "bin": {
    "svf": "./dist/index.js"
  },
  "scripts": {
    "start": "node ./node_modules/gulp/bin/gulp",
    "test": "nyc --reporter=text mocha",
    "prepublish": "npm test",
    "postinstall": "node ./dist/install",
    "gulp:scripts": "node ./node_modules/gulp/bin/gulp scripts",
    "gulp:static": "node ./node_modules/gulp/bin/gulp static",
    "gulp:watch": "node ./node_modules/gulp/bin/gulp watch"
  },
  "dependencies": {
    "archiver": "1.2.0",
    "bluebird": "3.4.6",
    "chalk": "1.1.3",
    "commander": "2.9.0",
    "crypto-js": "3.1.9-1",
    "dotenv": "4.0.0",
    "fs-extra": "2.1.2",
    "inquirer": "1.2.3",
    "jsforce": "1.7.1",
    "jsonfile": "2.4.0",
    "lodash": "4.17.2",
    "ngrok": "2.2.4",
    "node-watch": "0.4.1",
    "ora": "1.2.0",
    "pouchdb": "6.1.0",
    "pouchdb-find": "0.10.4",
    "randomstring": "1.1.5",
    "rollbar": "0.6.6"
  },
  "devDependencies": {
    "@types/archiver": "0.15.37",
    "@types/bluebird": "3.5.2",
    "@types/chai": "3.5.0",
    "@types/chai-as-promised": "0.0.30",
    "@types/chalk": "0.4.31",
    "@types/commander": "2.3.31",
    "@types/crypto-js": "3.1.33",
    "@types/debug": "0.0.29",
    "@types/dotenv": "2.0.20",
    "@types/fs-extra": "2.0.0",
    "@types/gulp": "4.0.2",
    "@types/gulp-typescript": "2.13.0",
    "@types/inquirer": "0.0.32",
    "@types/lodash": "4.14.62",
    "@types/mocha": "2.2.40",
    "@types/ora": "0.3.31",
    "@types/pouchdb": "5.4.28",
    "@types/pouchdb-find": "0.10.0",
    "@types/randomstring": "1.1.5",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "debug": "2.3.3",
    "gulp": "3.9.1",
    "gulp-typescript": "3.1.6",
    "mocha": "3.2.0",
    "nyc": "10.0.0",
    "typescript": "2.3.2"
  }
}
